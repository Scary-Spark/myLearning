<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:id="loginBox"
      xmlns="http://javafx.com/javafx" spacing="10"
      alignment="CENTER"
      styleClass="login-box"
      fx:controller="_1_customerLogin._1_2_loginController">

    <Label text="Login" fx:id="loginLabel"/>

    <TextField fx:id="usernameField" promptText="Username" visible="false"/>
    <PasswordField fx:id="passwordField" promptText="Password" visible="false"/>

</VBox>
